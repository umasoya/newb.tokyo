{
    "root": true,
    "extends": "eslint:recommended",
    "rules": {
        "indent": [ 2, 4 ],
        "linebreak-style": [ 2, "unix" ],
        "semi": [ 2, "always" ],
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
    },
    "parserOptions": {
        "sourceType": "module"
    }
}
